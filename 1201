#include <stdio.h>

int main()
{
    int n, i, a=1, b=1;
    scanf("%d", &n);
    for(i=0; i<n*n; i++)
    {
        if(b==n)
        {
            printf("%d\n", a);
            a++;
            b=1;
        }
        else
        {
            printf("%d ", a);
            a++;
            b++;
        }
    }
}
